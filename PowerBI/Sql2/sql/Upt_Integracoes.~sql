Create Table Niff_Integracoes (
             Origem Varchar2(3),
             Destino Varchar2(3),
             Data Date,
             TipoOperacao Varchar2(1),
             usuario Varchar2(15),
             CodigoEmpresa Number(3),
             CodigoFl Number(3),
             CodigoIntegrado Number) Tablespace Globus_Table;
                     